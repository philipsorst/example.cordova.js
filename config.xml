<?xml version='1.0' encoding='utf-8'?>
<widget id="net.dontdrinkandroot.cordovaexample" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
    <name>CordovaExample</name>
    <description>
        A sample Apache Cordova application that demonstrates Angular with Material Design
    </description>
    <author email="philip@sorst.net" href="https://sorst.net">
        Philip Washington Sorst
    </author>
    <content src="index.html"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
        <preference name="loadUrlTimeoutValue" value="900000"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
    <engine name="android" spec="^6.3.0"/>
    <engine name="browser" spec="^5.0.1"/>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2"/>
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3"/>
    <plugin name="cordova-plugin-vibration" spec="^2.1.5"/>
</widget>
